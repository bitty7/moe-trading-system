# Environment variables for the MoE trading system

# Required variables
DATA_PATH=../dataset/HS500-samples
LLM_MODEL_NAME=llama3.2:latest
BACKTEST_START_DATE=2000-01-01

# Optional variables with defaults
BACKTEST_END_DATE=2025-01-01
INITIAL_CAPITAL=1000000
POSITION_SIZING=0.15
MAX_POSITIONS=5
CASH_RESERVE=0.2
MIN_CASH_RESERVE=0.1
TRANSACTION_COST=0.001
SLIPPAGE=0.0005

# System configuration
LOG_LEVEL=WARNING
CACHE_TTL=3600
MAX_MEMORY_MB=4096
BATCH_SIZE=10
MAX_WORKERS=4
TIMEOUT_SECONDS=30
RETRY_ATTEMPTS=3

# Ollama configuration
OLLAMA_BASE_URL=http://localhost:11434
